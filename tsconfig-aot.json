{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "src/frontend"
  ],
  "files": [
    "src/.tmpProd/components/app.module.ts",
    "src/.tmpProd/main.ts"
  ],
  "angularCompilerOptions": {
    "genDir": "src/.aot",
    "skipMetadataEmit": true
  },
  "typeRoots": [
    "./node_modules/@types"
  ]
}
